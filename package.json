    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "lerna": "^7.4.2",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
